/*---------------Печать АКТа списания основных средств------------------------*/
import print_act;

file mvm (invmvmc,"bsmeans.def") key 0;

macro АКТ_СПИСАНИЯ(Mvm_FormPrint, ID)
  mvm.InvCard_Id  = ID;
  if (GetEQ(mvm))
    PrintAct(2, ID mvm.InvNumb);
  else
    MsgBox("Акт не найден!");
  end;
end;