/********** Определяем номер клиента по счету **********/
macro F_clt(ac)
 if(SubStr(ac, 6, 3) == "810")
  rewind(acc_r);
  ClearRecord(acc_r);
  acc_r.Account=ac;
  GetEQ(acc_r);
  return acc_r.Client;
 else
  rewind(acc_c);
  ClearRecord(acc_c);
  acc_c.Account=ac;
  acc_c.Code_Currency=SubStr(ac, 6, 3);
  GetEQ(acc_c);
  return acc_c.Client;
 end;
end;
/******************************************************/

/********** Определяем наименование счета Внебаланс **********/
macro GetAccountName_v(cod,ac)
 cod=Int(cod);
 if((cod==0) OR (cod==810))
  rewind(acc_v);
  ClearRecord(acc_v);
  acc_v.Chapter=3;
  acc_v.Account=ac;
  GetEQ(acc_v);
  return acc_v.NameAccount;
 else
  rewind(acc_vc);
  ClearRecord(acc_vc);
  acc_vc.Chapter=3;
  acc_vc.Account=ac;
  acc_vc.Code_Currency=cod;
  GetEQ(acc_vc);
  return acc_vc.NameAccount;
 end;
end;
