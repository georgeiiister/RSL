/********** Определяем номер счета по клиенту Внебалансовый **********/
macro GetAccountNumbN(n_clt,code,balance)
 flag = 0;
 cod=Int(code);
 clt=n_clt;
 bal=balance;
 if((cod==0) OR (cod==810))
   rewind(ob_r);
   ClearRecord(ob_r);
   ob_r.Chapter=3;
   ob_r.Open_Close="";
   ob_r.Client=clt;
   ob_r.Sort="";
   flag = GetGE(ob_r);
   while((flag))
       if(flag and (ob_r.Open_Close != "З") and (ob_r.Balance == bal) and (SubStr(ob_r.Account,6,3) == "810"))
         flag=0;
         return ob_r.Account;
       else
         flag=next(ob_r);
       end;
   end;
 else
   rewind(ob_c);
   ClearRecord(ob_c);
   ob_c.Chapter=3;
   ob_c.Open_Close="";
   ob_c.Client=clt;
   ob_c.Sort="";
   flag = GetGE(ob_c);
   while((flag))
       if(flag and (ob_c.Open_Close != "З") and (ob_c.Balance == bal) and (SubStr(ob_c.Account,6,3) == cod) and (ob_c.Code_Currency == cod))
         flag=0;
         return ob_c.Account;
       else
         flag=next(ob_c);
       end;
   end;
 end;
end;
